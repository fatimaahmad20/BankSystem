@model BankSys.Models.ApplicationUser
@{
    ViewBag.Title = "Home";
}


<h2>bank system</h2>
<div class="d-flex" style="transform: translateX(30%)">
    <img style="width:400px;" alt="piuc" src="data:image/jpeg;base64," />
</div>
<div style="display:flex;flex-direction:row;justify-content:space-between;">
    <div class="d-flex flex-column">
        <a class="btn btn-block bg-primary" href="/Bank/withdrawl/@Model.Id" style="width:450px; ">
            withdrawl

        </a>
        
        <a class="btn btn-block bg-primary" href="/Bank/Withdrawl100/@Model.Id" style="width:450px;">
            quick cash $100

        </a>
       
        <a class="btn btn-block bg-primary" href="/Bank/BalanceInquery/@Model.Id" style="width:450px;">
            balance inquiry

        </a>

    </div>
    <div class="d-flex flex-column">
        <a class="btn btn-block bg-primary" href="/Bank/Detials/@Model.Id" style="width:450px;">
            print statment

        </a>
        <a class="btn btn-block bg-primary" href="/Bank/Transfer/@Model.Id" style="width:450px;">
            transfer funds

        </a>
        <a class="btn btn-block bg-primary" href="/Bank/Deposit/@Model.Id" style="width:450px;">
            deposit

        </a>
    </div>
</div>
